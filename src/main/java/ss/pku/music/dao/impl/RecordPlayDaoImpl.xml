<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ss.pku.music.dao.RecordPlayDao">

	<insert id="insert"
		parameterType="ss.pku.music.model.PlayRecord">
		insert into play(userId,songId) values(#{userId},#{songId})
	</insert>
	
	<select id="selectAll"
		resultType="ss.pku.music.model.PlayRecord">
		select * from play
	</select>

</mapper>